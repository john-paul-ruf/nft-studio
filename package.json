{
  "name": "nft-studio",
  "version": "0.0.1",
  "description": "A front end for my-nft-gen",
  "main": "main.js",
  "scripts": {
    "test": "node tests/runner.js",
    "test:verify": "node test-verify.js",
    "test:app": "node tests/runner.js App",
    "test:canvas": "node tests/runner.js Canvas",
    "test:wizard": "node tests/runner.js ProjectWizard",
    "test:services": "node tests/runner.js services",
    "test:ipc": "node tests/runner.js ipc",
    "test:prefs": "node test-preferences.js",
    "test:effects": "node test-effect-fix.js",
    "test:config": "node test-config-introspection-comprehensive.js",
    "test:integration": "node tests/integration/effect-dialog-integration.test.js",
    "test:regression": "node tests/regression/dialog-fixes.test.js",
    "test:unit": "node tests/unit/effect-configurer.test.js",
    "test:effect-defaults": "node tests/unit/effect-defaults.test.js",
    "test:ipc-serialization": "node tests/unit/ipc-serialization.test.js",
    "test:effect-name-lookup": "node tests/unit/effect-name-lookup.test.js",
    "test:percentage-range": "node tests/unit/percentage-range-config.test.js",
    "test:render-frame": "node tests/unit/render-single-frame.test.js",
    "test:effect-validation": "node tests/integration/effect-config-validation.test.js",
    "test:dialog-fixes": "npm run test:regression && npm run test:unit && npm run test:effect-defaults && npm run test:integration",
    "start": "webpack && npm run copy:html && electron .",
    "start:dev": "webpack --mode=development && npm run copy:html && electron .",
    "dev": "webpack --watch",
    "build": "webpack --mode=production && npm run copy:html",
    "copy:html": "cp dist/index.html dist/index.html 2>/dev/null || cp index.template.html dist/index.html",
    "electron": "electron .",
    "rebuild": "electron-rebuild -f -w canvas && electron-rebuild -f -w sharp",
    "clean:start": "rm -rf dist/ && webpack && npm run copy:html && electron ."
  },
  "author": "john.paul.ruf",
  "license": "MIT",
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.2",
    "@mui/material": "^7.3.2",
    "my-nft-gen": "file:../my-nft-gen",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.24.5",
    "@babel/preset-env": "^7.24.5",
    "@babel/preset-react": "^7.24.1",
    "@electron/rebuild": "^3.6.0",
    "babel-loader": "^9.1.3",
    "css-loader": "^6.8.1",
    "electron": "^30.0.6",
    "style-loader": "^3.3.3",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4"
  },
  "build": {
    "asar": true,
    "asarUnpack": [
      "node_modules/ffmpeg-static/bin/${os}/${arch}/ffmpeg",
      "node_modules/ffmpeg-static/index.js",
      "node_modules/ffmpeg-static/package.json",
      "**/node_modules/sharp/**/*",
      "**/node_modules/@img/**/*"
    ]
  }
}
