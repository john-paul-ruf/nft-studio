{
  "name": "nft-studio",
  "version": "0.0.1",
  "description": "A front end for my-nft-gen",
  "main": "main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "webpack && electron .",
    "dev": "webpack --watch",
    "build": "webpack --mode=production",
    "electron": "electron .",
    "rebuild": "electron-rebuild -f -w canvas && electron-rebuild -f -w sharp"
  },
  "author": "john.paul.ruf",
  "license": "MIT",
  "dependencies": {
    "my-nft-gen": "file:../my-nft-gen",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.24.5",
    "@babel/preset-env": "^7.24.5",
    "@babel/preset-react": "^7.24.1",
    "@electron/rebuild": "^3.6.0",
    "babel-loader": "^9.1.3",
    "css-loader": "^6.8.1",
    "electron": "^30.0.6",
    "style-loader": "^3.3.3",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4"
  },
  "build": {
    "asar": true,
    "asarUnpack": [
      "node_modules/ffmpeg-static/bin/${os}/${arch}/ffmpeg",
      "node_modules/ffmpeg-static/index.js",
      "node_modules/ffmpeg-static/package.json",
      "**/node_modules/sharp/**/*",
      "**/node_modules/@img/**/*"
    ]
  }
}
